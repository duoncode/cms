{
    "extends": "./.svelte-kit/tsconfig.json",
    "compilerOptions": {
        "allowSyntheticDefaultImports": true,
        "paths": {
            "$lib": ["./src/lib"],
            "$lib/*": ["./src/lib/*"],
            "$types": ["./src/types"],
            "$types/*": ["./src/types/*"],
            "$shell": ["./src/shell"],
            "$shell/*": ["./src/shell/*"]
        }
    }
}
