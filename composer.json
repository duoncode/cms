{
	"name": "duon/cms",
	"description": "Duon content management system and framework",
	"type": "library",
	"license": "MIT",
	"authors": [
		{
			"name": "ebene fÃ¼nf GmbH",
			"email": "info@ebenefuenf.de"
		}
	],
	"minimum-stability": "dev",
	"prefer-stable": true,
	"require": {
		"php": ">=8.3",
		"duon/core": "dev-main",
		"duon/error": "dev-main",
		"duon/quma": "dev-main",
		"duon/registry": "dev-main",
		"duon/session": "dev-main",
		"duon/sire": "dev-main",
		"gumlet/php-image-resize": "2.0.*",
		"hidehalo/nanoid-php": "^2",
		"laminas/laminas-httphandlerrunner": "^2.4",
		"psr/http-message-implementation": "^1.0",
		"symfony/html-sanitizer": "^7.1"
	},
	"require-dev": {
		"duon/cms-boiler": "dev-main",
		"duon/dev": "^1.1",
		"laminas/laminas-diactoros": "^3",
		"phpunit/phpunit": "^12"
	},
	"autoload": {
		"psr-4": {
			"Duon\\Cms\\": "src"
		},
		"files": [
			"src/Util/Functions.php",
			"src/Field/Meta/Functions.php"
		]
	},
	"autoload-dev": {
		"psr-4": {
			"Duon\\Cms\\Tests\\": "tests"
		}
	},
	"config": {
		"allow-plugins": {
			"dealerdirect/phpcodesniffer-composer-installer": true
		}
	}
}
